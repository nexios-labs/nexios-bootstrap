# Database Configuration
DATABASE_URL=postgres://postgres:password@localhost:5432/nexios

# Or use individual PostgreSQL settings
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=password
DB_NAME=nexios

# Application Settings
APP_ENV=development
DEBUG=true
SECRET_KEY=your-secret-key-here

# CORS Settings
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Redis (if enabled)
REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/app.log
